CREATE TABLE USER_DATA(
	 NAME VARCHAR(30) NOT NULL,
	 AGE INTEGER(3) NOT NULL,
	 JOB VARCHAR(52) NOT NULL,
	 CAR_YN VARCHAR(2) NOT NULL DEFAULT "N",
	 HOME_YN VARCHAR(2) NOT NULL DEFAULT "N",
	 MARRIAGE_YN VARCHAR(2) NOT NULL DEFAULT "N",
	 HOBBY VARCHAR(100),
	 PRIMARY KEY(NAME,AGE)
)ENGINE=InnoDB DEFAULT CHARSET=utf8mb4;





CREATE TABLE USER_INFO(
	 NAME VARCHAR(30) NOT NULL,
	 AGE INTEGER(3) NOT NULL,
	 ADDR VARCHAR(200) ,
	 PHONE VARCHAR(50)  NOT NULL,
	 PRIMARY KEY(NAME , AGE)
)ENGINE=InnoDB DEFAULT CHARSET=utf8mb4;

